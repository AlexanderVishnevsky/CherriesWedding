{
    "name": "cherrys-wedding",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "echo \"---> LINT: \" && pnpm tsc --build --clean && pnpm eslint . --quiet --fix && pnpm prettier --write .",
        "post-update": "echo \"---> DEPENDENCES UPDATE: \" && pnpm upgrade --latest",
        "pre-commit": "pnpm run lint && pnpm run build",
        "prod-run": "pnpm run build && pnpm run start",
        "serve": "next start",
        "postinstall": "husky install",
        "webpack-analyze": "ANALYZE=true next build"
    },
    "dependencies": {
        "@emotion/cache": "^11.11.0",
        "@emotion/react": "^11.11.1",
        "@emotion/server": "^11.11.0",
        "@emotion/styled": "^11.11.0",
        "@mui/material": "^5.14.18",
        "@mui/system": "^5.14.18",
        "axios": "^1.6.2",
        "googleapis": "^128.0.0",
        "next": "^14.0.3",
        "next-pwa": "^5.6.0",
        "next-translate": "^2.6.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "zustand": "^4.4.6"
    },
    "devDependencies": {
        "@babel/plugin-transform-private-property-in-object": "^7.23.3",
        "@next/bundle-analyzer": "^14.0.3",
        "@next/eslint-plugin-next": "^14.0.3",
        "@svgr/webpack": "^8.1.0",
        "@types/node": "^20.9.2",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@typescript-eslint/eslint-plugin": "^6.11.0",
        "@typescript-eslint/parser": "^6.11.0",
        "eslint": "^8.54.0",
        "eslint-config-next": "^14.0.3",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-flowtype": "^8.0.3",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-unused-imports": "^3.0.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.1.0",
        "next-translate-plugin": "2.6.2",
        "prettier": "^3.1.0",
        "typescript": "^5.2.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.(ts|tsx)": "pnpm tsc --noEmit",
        "*.(ts|tsx|js)": [
            "pnpm eslint --fix ${filenames.join(' ')}",
            "pnpm prettier --write ${filenames.join(' ')}"
        ],
        "**/*.(md|json)": "pnpm prettier --write ${filenames.join(' ')}"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
