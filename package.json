{
    "name": "cherrys-wedding",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "echo \"---> LINT: \" && pnpm tsc --build --clean && pnpm eslint . --fix && pnpm prettier --write .",
        "post-update": "echo \"---> DEPENDENCES UPDATE: \" && pnpm upgrade --latest",
        "pre-commit": "pnpm run lint && pnpm run build",
        "prod-run": "pnpm run build && pnpm run start",
        "serve": "next start",
        "postinstall": "husky install",
        "webpack-analyze": "ANALYZE=true next build"
    },
    "dependencies": {
        "@emotion/cache": "^11.11.0",
        "@emotion/react": "^11.11.4",
        "@emotion/server": "^11.11.0",
        "@emotion/styled": "^11.11.5",
        "@mui/material": "^5.15.15",
        "@mui/system": "^5.15.15",
        "axios": "^1.6.8",
        "googleapis": "^134.0.0",
        "next": "^14.2.0",
        "next-pwa": "^5.6.0",
        "next-translate": "^2.6.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "zustand": "^4.5.2"
    },
    "devDependencies": {
        "@babel/plugin-transform-private-property-in-object": "^7.24.1",
        "@next/bundle-analyzer": "^14.2.0",
        "@next/eslint-plugin-next": "^14.2.0",
        "@svgr/webpack": "^8.1.0",
        "@types/node": "^20.12.7",
        "@types/react": "^18.2.77",
        "@types/react-dom": "^18.2.25",
        "@typescript-eslint/eslint-plugin": "^7.6.0",
        "@typescript-eslint/parser": "^7.6.0",
        "eslint": "8.57.0",
        "eslint-config-next": "^14.2.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-flowtype": "^8.0.3",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-unused-imports": "^3.1.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "next-translate-plugin": "2.6.2",
        "prettier": "^3.2.5",
        "typescript": "^5.4.5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.(ts|tsx)": "pnpm tsc --noEmit",
        "*.(ts|tsx|js)": [
            "pnpm eslint --fix ${filenames.join(' ')}",
            "pnpm prettier --write ${filenames.join(' ')}"
        ],
        "**/*.(md|json)": "pnpm prettier --write ${filenames.join(' ')}"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
